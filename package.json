{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "tsnd -r dotenv/config --exit-child src/main/server.ts",
    "db:seed": "yarn prisma db seed",
    "db:migrate": "yarn prisma migrate deploy",
    "db:migrate:dev": "yarn prisma migrate dev",
    "db:view": "npx prisma studio",
    "jest": "NODE_ENV=test jest --max-old-space-size=8192 --passWithNoTests --runInBand --no-cache --no-coverage",
    "test": "yarn jest --force-exit",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --onlyChanged",
    "test:clearCache": "yarn jest --clearCache",
    "test:coverage": "yarn jest --coverage --silent --logHeapUsage",
    "build": "tsc"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@jest-mock/express": "^2.0.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.0.3",
    "@types/module-alias": "^2.0.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "dotenv": "^16.0.2",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^29.0.3",
    "jest-mock": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "prettier": "^2.3.1",
    "prisma": "^4.3.1",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.3"
  },
  "engines": {
    "node": "18.x"
  }
}
